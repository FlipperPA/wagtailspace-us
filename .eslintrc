{
  "extends": "airbnb",
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "comma-dangle": 0,
    "max-len": 0,
    "arrow-parens": 0,
    "arrow-body-style": 0,
    "no-confusing-arrow": 0,
  },
  "globals": {
    "document": true,
    "window": true,
    "fetch": true,
  },
  "parser": "babel-eslint"
}
